
<h1>Create Your Post</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @post.each do |post| %>
      <tr>
        <td><%= post.body %></td>
        <td><%= show_right_user %><td>
      </tr>
    <% end %>

      <%= sign_in_or_out %>
        
        
  </tbody>
</table>

<br>

<%= link_to 'New Post', new_post_path %>